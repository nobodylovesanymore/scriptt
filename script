        fireclickdetector(game.Workspace.Ignored.Shop["200 [LMG Ammo] - $328"].ClickDetector)
        wait(0.5)
        humanoid.Parent:SetPrimaryPartCFrame(CFrame.new(originalPosition))
    end
end)

